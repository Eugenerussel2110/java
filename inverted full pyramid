public class InvertedPyramid{
  public static void main(String[] args){
    int num = 6;
    for(int i=num;i>0;i--){
      for(int j=0;j<num-i;j++){
        System.out.print(" ");
      }
      for(int j=0;j<(2*i-1);j++){
        System.out.print("*");
      }
      System.out.println();
    }
  }
}
